	"authors": [
		"Arclight!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/programmableminecarts/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.arclight.programmableminecarts.ProgrammableMinecarts"
		],
		"fabric-datagen": [
			"net.arclight.programmableminecarts.ProgrammableMinecartsDataGenerator"
		],
		"client": [
			"net.arclight.programmableminecarts.ProgrammableMinecartsClient"
		]
	},
	"mixins": [
		"programmableminecarts.mixins.json"
	],
	"depends": {
